<div class="container">
    <div class="jumbotron">
        <h1>Lets scrap some articles</h1>
    </div>

    <div class="row">
        <div class="col-xs-8 articles">

            {{#each doc}}

                <p id="article-content"><a href="{{this.link}}"><h2>{{this.title}}</h2></a></p>
                <p>
                <h5 id="article-content">{{this.articleNote}}</h5></p>

                <img src="{{this.image}}">

            {{/each}}

        </div>
    </div>

</div>