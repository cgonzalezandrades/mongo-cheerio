<div class="container">
  <div class="jumbotron">
    <div class="row">
      <h1>Lets scrap some articles from NYT</h1>
    </div>
    <div class="row">

      <div class="col-md-6">
      
<a href="https://aqueous-falls-53542.herokuapp.com/scrape"><button id="buttons" type="button" class="btn btn-success">Scrape Articles</button></a>
        
<!--        https://aqueous-falls-53542.herokuapp.com/scrape-->
        
      </div>
      <div class="col-md-6"></div>
<a href="https://aqueous-falls-53542.herokuapp.com/delete"><button id="buttons" type="button" class="btn btn-danger">Scrape Articles</button></a>
    </div>
  </div>

  <div class="container">
  <div class="row">
    <div class="col-xs-12 articles">

      {{#each doc}}

      <p id="article-content"><a href="{{this.link}}"><h2>{{this.title}}</h2></a></p>
      <p>
        <h4 id="article-content">{{this.articleNote}}</h4></p>

      <img src="{{this.image}}">
      <hr> {{/each}}


    </div>
  </div>
</div>



</div>